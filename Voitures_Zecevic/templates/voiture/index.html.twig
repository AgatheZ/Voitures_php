{% extends 'base.html.twig' %}

{% block title %}Voiture index{% endblock %}

{% block body %}
    <h1 class="display-4 text-center mt-2 mb-5">Liste des voitures</h1>

    {# <table class="table">
        <thead>
            <tr>
                <th>Identifiant</th>
                <th>Immatriculation</th>
                <th>Marque</th>
                <th>Carburant</th>
                <th>Nombre de places</th>
                <th>Actions</th>
            </tr>
        </thead>
       
    </table> #}


    {% for voiture in voitures %}
    <div class="row col-sm-6">
        <div class="col">
            <div class="card border-info border-width-5 mb-3 "style="width: 18rem; length: 18rem;"> <img src=https://image.freepik.com/vecteurs-libre/image-tridimensionnelle-voiture-verte-isolee-fond-blanc_53876-40876.jpg class="card-img-top" width="100%">
                <div class="card-body pt-0 px-0" >
                    <div class="d-flex flex-column justify-content-between px-3 pb-3"> <center><small class="text-muted"><td><h6> Voiture n°{{voiture.id }}</center></h6></td></small></div>
                    <hr class="mt-2 mx-2">
                    <div class="d-flex flex-row justify-content-between p-3 mid">
                        <div class="d-flex flex-column"><small class="text-muted mb-3"><th>Marque</th> <h6><td>{{voiture.marque}}</td></h6></small>
                            <div class="d-flex flex-row"><img src="data:image/jpeg;base64," width="35px" height="35px">
                                <div class="d-flex flex-column ml-1"><small class="ghj">Fonctionne à l'{{ voiture.carburant }}</small></div>
                            </div>
                        </div>
                        <div class="d-flex flex-column"><small class="text-muted mb-2"><td><th>Immatriculation</th> <h6><td>{{ voiture.immatriculation }}</td></h6></td></small>
                            <div class="d-flex flex-row"><i class="bi bi-chat-quote text-info fs-2"></i>
                                <h7 class="ml-1"><td>{{ voiture.nombrePlaces }} places*</td></h7>
                            </div>
                        </div>
                    </div> 
                    <div class="d-flex flex-row justify-content-between p-3 mid">
                       <td> 
                        <div class="d-flex flex-column"><a type="submit" class="btn btn-secondary btn-block" href="{{path('voiture_edit', {'id': voiture.id})}}">Modifier</a></div>
                        {{ include('voiture/_delete_form.html.twig') }}
                        </td>
                </div>
                </div>
            </div>
        </div>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}

       
    </div>

{% endblock %}
