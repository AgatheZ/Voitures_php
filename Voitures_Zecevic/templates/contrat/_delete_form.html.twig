<form method="post" action="{{ path('contrat_delete', {'id': contrat.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ contrat.id) }}">
    <button title="Supprimer le contrat" class="btn btn-link" style="padding: 0">
        <i class="bi bi-trash-fill"></i>
</form>
